{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "tempJSON",
      "type": "table",
      "tableName": "temperatures",
      "rowKey": "{date}",
      "connection": "AzureWebJobsStorage",
      "direction": "in"
    },
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ],
      "route": "temp/{date}"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}
